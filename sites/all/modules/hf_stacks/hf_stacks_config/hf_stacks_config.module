<?php
/**
 * Implements hook_menu()
 * @return array
 */
function hf_stacks_config_menu() {
  $items = array();
  $items['admin/config/stacks/configuration'] = array(
    'title' => t('Stacks Configuration'),
    'page callback' => 'drupal_get_form',
    'page arguments' => array('hf_stacks_config_form'),
    'access arguments' => array('administer stacks config'),
    'type' => MENU_CALLBACK,
    'file' => 'hf_stacks_config.admin.inc'
  );
  return $items;
}