    margin: .5cm;
}

  p,
  h2,
  h3 {
    orphans: 3;
    widows: 3;
  }

  h2,
  h3 {
    page-break-after: avoid;
  }

  .hide-on-print {
    display: none!important;
  }

  .print-only {
    display: block!important;
  }

  .hide-for-print {
    display: none!important;
  }

  .show-for-print {
    display: inherit!important;
  }
